<?xml version="1.0" encoding="UTF-8" ?><!-- $Id: install.xml 760 2012-05-30 03:12:19Z malu $ -->
<XMLDB PATH="blocks/sharing_cart/db" VERSION="2012053000" COMMENT="XMLDB file for Moodle sharing cart"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="sharing_cart" COMMENT="table to store sharing data" NEXT="sharing_cart_plugins">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="modname"/>
        <FIELD NAME="modname" TYPE="char" LENGTH="32" NOTNULL="true" DEFAULT="" SEQUENCE="false" ENUM="false" PREVIOUS="userid" NEXT="modicon"/>
        <FIELD NAME="modicon" TYPE="char" LENGTH="32" NOTNULL="true" DEFAULT="" SEQUENCE="false" ENUM="false" PREVIOUS="modname" NEXT="modtext"/>
        <FIELD NAME="modtext" TYPE="char" LENGTH="255" NOTNULL="true" DEFAULT="" SEQUENCE="false" ENUM="false" PREVIOUS="modicon" NEXT="ctime"/>
        <FIELD NAME="ctime" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="modtext" NEXT="filename"/>
        <FIELD NAME="filename" TYPE="char" LENGTH="255" NOTNULL="true" DEFAULT="" SEQUENCE="false" ENUM="false" PREVIOUS="ctime" NEXT="tree"/>
        <FIELD NAME="tree" TYPE="char" LENGTH="255" NOTNULL="true" DEFAULT="" SEQUENCE="false" ENUM="false" PREVIOUS="filename" NEXT="weight"/>
        <FIELD NAME="weight" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="tree"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" />
      </KEYS>
    </TABLE>
    
    <TABLE NAME="sharing_cart_plugins" COMMENT="table to store sharing cart plugins data" PREVIOUS="sharing_cart">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="plugin"/>
        <FIELD NAME="plugin" TYPE="char" LENGTH="32" NOTNULL="true" DEFAULT="" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="plugin" NEXT="data"/>
        <FIELD NAME="data" TYPE="text" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="userid"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" />
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
